Algoritmo "semnome"

Var

hora: inteiro;

Inicio



 escreval("Informe a hora: ")
 leia(hora)

 se (hora <= 12) entao
 escreval("Bom dia!")



 senao
      se (hora <=18)entao
      escreval("Boa tarde!")
      
 senao
      escreval("Boa noite!")


      fimse


 fimse

Fimalgoritmo